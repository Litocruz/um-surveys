%div.page-header
  %h1.pull-left Resultados
  %br
  %br
%ul.nav.nav-tabs
  %li.active= link_to "JSON Results", "#json-results", "data-toggle" => "tab"
  %li= link_to "HTML Results", "#html-results", "data-toggle" => "tab"

.tab-content
  .tab-pane.active#json-results
    = render partial: "json_results", locals: { survey: @survey }
  .tab-pane#html-results
    = render partial: "html_results", locals: { results: @survey_results }